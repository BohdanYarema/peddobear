@media (min-aspect-ratio: 16/9)
    #video
        height 300%
        top -100%

@media (max-aspect-ratio: 16/9)
    #video
        width 300%
        left -100%

@supports (object-fit: cover)
    #video
        top 0
        left 0
        width 100%
        height 100%
        object-fit cover
        font-family 'object-fit: cover;'